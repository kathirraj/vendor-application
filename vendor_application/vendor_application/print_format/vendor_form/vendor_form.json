 "html": "<html>\n    <style>\n        table,th,td {\n            border-bottom:none;\n            border-collapse: collapse;\n            font-size:9px; \n        }\n        .body {\n            width: 100%;\n            height: auto;\n            border: 5px solid black;\n            border-radius: 1px;\n            background: #f3f3f3;\n            margin-bottom: '10px';\n            box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);\n        }\n        \n        h2,h3 {\n            margin-top:0px!important;\n            margin-bottom:0px!important;\n        }\n        \n        .table-head {\n            font-size: large;\n            text-align:left;\n        }\n        \n        .table-body {\n            padding-left: 8px;\n            padding-right: 8px;\n        }\n        \n        .company-contact td,th {\n            border: 1px solid rgb(0, 0, 0);\n            padding: 10px;\n        }\n\n        * {\n            box-sizing: border-box;\n            -moz-box-sizing: border-box;\n        }\n        \n        .print-format td {\n            vertical-align:unset!important;\n            padding-bottom:3px!important;\n            padding-top:3px!important;\n            padding-left:3px!important;\n            padding-right:0px!important;\n        }\n        \n        .print-format th {\n            padding:4px!important;\n        }\n        .body {\n            min-height: 25cm;\n        }\n        @media print {\n            .body {\n                size: A4;\n                margin: 0;\n                padding: 0;\n                background-color: #f3f3f3;\n            }\n            @top-left {\n                content: element(header);\n            }\n        }\n\n    </style>\n    <div class=\"body\">\n        <div class=\"header\" id=\"header\">\n            <header style=\"padding-left:10px; padding-right:10px;\">\n                <table class=\"table-head\" style=\"width:100%; border-bottom:1px solid black;\">\n                    <tr>\n                        <td colspan=\"10\"><h2>VENDOR REGISTRATION FORM</h2></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"8\"><h3>COMPANY NAME</h3></td>\n                        <td rowspan=\"5\" colspan=\"2\" style=\"text-align:center;\"><h3>COMPANY LOGO</h3></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"8\"><b>Address Line 1</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"8\"><b>Address Line 2</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"8\"><b>City</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"8\"><b>State, Postal pin : </b></td>\n                    </tr>\n                </table>\n            </header>\n        </div>\n        <div class=\"vendor\">\n            <main class=\"table-body\">\n                <table class=\"company-contact\" style=\"width:100%;\">\n                    <tr>\n                        <td colspan=\"10\" style=\"letter-spacing:1px; border:none!important; padding:0px!important; font-style: italic;\">\n                            <br><b>VENDOR DETAILS</b>\n                        </td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>VENDOR ID : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.name }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>VENDOR NAME : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.vendor_name }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>NATURE OF VENDOR : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.nature_of_vendor }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>DATE OF INCORPORATION : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.date_of_incorporation }}</b></td>\n                    </tr>\n                    <tr style=\"height:10px;\">\n                        <td colspan=\"10\" style=\"letter-spacing:1px; border:none!important; padding:0px!important; font-style: italic;\">\n                            <br><b>ADDRESS</b>\n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td colspan=\"1\"><b>CONTACT ADDRESS</b></td>\n                        <td colspan=\"9\"><b>{{ doc.address_line_1 }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>CITY : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.city }}</b></td>\n                        \n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>POSTAL CODE : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.pin_code }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>STATE : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.state }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>COUNTRY : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.country }}</b></td>\n                    </tr>\n                    \n                    <tr>\n                        <td colspan=\"10\" style=\"letter-spacing:1px; border:none!important; padding:0px!important; font-style: italic;\">\n                            <br><b>MATERIAL INFO</b>\n                        </td>\n                    </tr>\n                    <tr>\n                        {%- for row in doc.materials -%}\n                        <td colspan=\"1\"><b>PARTICULARS OF MATERIAL : </b></td>\n                        <td colspan=\"3\"><b>{{ row.material }}</b></td>\n                        <td colspan=\"3\"><b>{{ row.materials }}</b></td>\n                        <td colspan=\"3\"><b>{{ row.material_type }}</b></td>\n                        {%- endfor -%}\n                    </tr>\n                    <tr>\n                        <td colspan=\"10\" style=\"letter-spacing:1px; border:none!important; padding:0px!important; font-style: italic;\">\n                            <br><b>PAYMENT TERMS</b>\n                        </td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>CREDIT PERIOD : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.credit_period }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>CURRENCY : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.currency }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"10\" style=\"letter-spacing:1px; border:none!important; padding:0px!important; font-style: italic;\">\n                            <br><b>FINANCIAL DETAILS</b>\n                        </td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>Current Financial Year & DATE : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.financial_year }} & {{ doc.required_date }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>Last Financial Year & DATE : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.last_financial_year }} & {{ doc.date }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"10\" style=\"letter-spacing:1px; border:none!important; padding:0px!important; font-style: italic;\">\n                            <br><b>CONTACT INFO</b>\n                        </td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>CONTACT PERSON : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.contact_person }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>TELEPHONE NO : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.telephone_no }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>FAX : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.fax_no }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>EMAIL ADDRESS : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.email_address }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"10\" style=\"letter-spacing:1px; border:none!important; padding:0px!important; font-style: italic;\">\n                            <br><b>REGISTERED DETAIL</b>\n                        </td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>GSTIN NUMBER : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.gstin_no }}</b></td>\n                    </tr>\n                    <tr>    \n                        <td colspan=\"1\"><b>PAN NUMBER : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.pan_no }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"10\" style=\"letter-spacing:1px; border:none!important; padding:0px!important; font-style: italic;\">\n                            <br><b>BANK INFORMATION</b>\n                        </td>\n                    </tr>\n                    <tr>    \n                        <td colspan=\"1\"><b>BANK NAME : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.name_of_bank }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>ACCOUNT NO : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.account_number }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>BRANCH NAME : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.branch }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>SWIFT CODE : </b></td>\n                        <td colspan=\"4\"><b>{{ doc.swift_code }}</b></td>\n                        <td colspan=\"1\"><b>IFSC CODE : </b></td>\n                        <td colspan=\"4\"><b>{{ doc.ifsc_code }}</b></td>\n                    </tr>\n                    \n                    <tr>\n                        <td colspan=\"10\" style=\"letter-spacing:1px; border:none!important; padding:0px!important; font-style: italic;\">\n                            <br><b>MSME REGISTERED INFO</b>\n                        </td>\n                    </tr>\n                    <tr>    \n                        <td colspan=\"1\"><b>MSME REGISTERED : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.msme_reg }}</b></td>\n                    </tr>\n                    <tr>\n                        {% if (doc.msme_reg == \"YES\") %}\n                        <td colspan=\"1\"><b>REGISTERED : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.registered }}</b></td>\n                    </tr>\n                    <tr>\n                        <td colspan=\"1\"><b>COPY OF CERTIFICATE : </b></td>\n                        <td colspan=\"9\"><b>{{ doc.copy_of_certificate_enclosed }}</b></td>\n                        {% endif %}\n                    </tr>\n                </table>\n            </main>\n        </div>\n        <div class=\"footer\" id=\"footer\"></div>\n    </div>\n</html>",
